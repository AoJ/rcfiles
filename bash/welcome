date

async_check_rcfiles() {
  {
    local RCFILES_UPDATED=$(echo $(cd ~/dev/rcfiles && git ls-remote origin && git ls-remote .) | tr ' ' '\n' | sed 's/[a-zA-Z/]*//' | sed '/^$/d' | uniq | wc -l | awk '{print $1==1}')
    if [[ $RCFILES_UPDATED == 0 ]]
      then echo -e "\rRCFILES repo is not up-to date."
    fi
  }&
}

async_check_rcfiles
disown
